<%- include('layouts/layout.top.ejs') %>

<table
  width="600px"
  class="table__main"
  align="center"
  cellpadding="0"
  cellspacing="0"
  bgcolor="white"
>
  <tr>
    <td>
      <p style="margin: 15px 0 12px 0; font-weight: bold; text-align: center">
        Sign-in Method Changed
      </p>
    </td>
  </tr>
  <tr>
    <td>
      <div>
        Hello <%= user.first_name %>. Your Dome sign-in method has been
        successfully changed to <% if (user.auth_type === 'GOOGLE') { %>
        Email/Password with <%= user.email %> being the point of reference
        email. <%} else { %> Google sign-in. <% } %>
      </div>
    </td>
  </tr>
</table>

<%- include('layouts/layout.bottom.ejs') %>
